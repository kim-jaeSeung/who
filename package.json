  },
  "homepage": "https://github.com/kim-jaeSeung/who#readme",
  "description": "",
  "devDependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.5",
    "@babel/template": "^7.27.2",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "expect": "^30.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pretty-format": "^30.0.0",
